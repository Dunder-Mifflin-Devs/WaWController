  },
  "homepage": "https://github.com/Dunder-Mifflin-Devs/WaWuserManagementService#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "jest": "^29.6.3",
    "mongodb-memory-server": "^8.15.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
