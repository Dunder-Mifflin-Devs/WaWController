{
  "name": "wawusermanagementservice",
  "version": "1.0.0",
  "description": "Worth-a-Watch's User Management Service provides the API for login processes and also provides the API for more profile-related elements",
  "main": "src/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --testEnvironment=node --runInBand ./test --config=./test/jest.config.js",
    "test-coverage": "jest --testEnvironment=node --runInBand ./test --config=./test/jest.config.js --coverage",
    "start": "cd src && node server.js",
    "dev": "cd src && nodemon server.js ",
    "devLocal": "cross-env NODE_ENV=production cd src && nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dunder-Mifflin-Devs/WaWuserManagementService.git"
